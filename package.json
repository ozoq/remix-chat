{
  "private": true,
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./app/lib/tailwind/app.css -o app/lib/tailwind/build.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./app/lib/tailwind/app.css -o app/lib/tailwind/build.css",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@ngneat/falso": "^6.1.0",
    "@prisma/client": "^4.5.0",
    "@remix-run/node": "^1.7.4",
    "@remix-run/react": "^1.7.4",
    "@remix-run/serve": "^1.7.4",
    "daisyui": "^2.38.0",
    "isbot": "^3.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.7.4",
    "@remix-run/eslint-config": "^1.7.4",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.5.0",
    "eslint": "^8.23.1",
    "postcss": "^8.4.18",
    "prisma": "^4.5.0",
    "tailwindcss": "^3.2.1",
    "typescript": "^4.7.4"
  },
  "prisma": {
    "schema": "app/lib/prisma/schema.prisma",
    "seed": "ts-node app/lib/prisma/seed"
  }
}
